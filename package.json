{
  "name": "mini-inventory",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --coverage",
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node --project tsconfig.json src/frameworks/tui/app.ts",
    "serve": "node dist/frameworks/http/server.js",
    "dev:serve": "ts-node --project tsconfig.json src/frameworks/http/server.ts",
    "tui": "node dist/frameworks/tui/app.js",
    "dev:tui": "ts-node --project tsconfig.json src/frameworks/tui/app.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
